@typedef:.\Resources\phpdef.json
@ext:.php

<?php
    class @class_name
    {
        @repeat-col:var $@property_name;
        @repeat-col:function get@property_name(){
        @repeat-col:    return $this->@property_name;
        @repeat-col:}
        @repeat-col:function set@property_name($value){
        @repeat-col:    $this->@property_name = $value;
        @repeat-col:}
    }
?>